<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" layoutX="91.0" layoutY="108.0" prefHeight="160.0" prefWidth="463.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="321.0">
               <children>
                  <Label fx:id="UsernameLabel" prefHeight="27.0" prefWidth="110.0" text="Username:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="LogInUsernameText" prefHeight="25.0" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="PasswordLabel" prefHeight="27.0" prefWidth="110.0" text="Password:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="LogInPasswordText" prefHeight="25.0" prefWidth="200.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox alignment="CENTER" layoutX="23.0" layoutY="268.0" prefHeight="73.0" prefWidth="508.0">
         <children>
            <Button fx:id="LogInButton" mnemonicParsing="false" onAction="#OnActionLogIn" prefHeight="44.0" prefWidth="96.0" text="Log In">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <VBox alignment="CENTER" layoutX="23.0" layoutY="24.0" prefHeight="73.0" prefWidth="508.0">
         <children>
            <Label fx:id="PleaseLogInLabel" text="Please Log In">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
